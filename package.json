{
	"name": "@RabbitSwapDEX/koikoi-types",
	"version": "1.0.0",
	"description": "Shared type definitions for the Koikoi project",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --clean",
		"dev": "tsup src/index.ts --format esm,cjs --dts --watch"
	},
	"dependencies": {
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"tsup": "^8.2.3",
		"typescript": "^5.5.4"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RabbitSwapDEX/koikoi.git",
		"directory": "packages/types"
	},
	"keywords": [
		"koikoi",
		"types",
		"shared"
	],
	"license": "ISC"
}
